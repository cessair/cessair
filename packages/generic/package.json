{
    "name": "@cessair/generic",
    "version": "1.0.1",
    "description": "The interface for generic programming.",
    "keywords": [
        "cessair",
        "helper",
        "utility",
        "catalyst",
        "extension"
    ],
    "homepage": "https://cessair.github.io/",
    "bugs": {
