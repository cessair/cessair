    },
    "license": "MIT",
    "author": {
        "name": "Yongbin Min",
        "email": "contact@cichol.com",
        "url": "https://www.cichol.com/"
    },
    "main": "libraries",
    "directories": {
        "lib": "libraries",
        "test": "tests"
    },
    "repository": "https://github.com/cessair/cessair/tree/master/packages/generic",
    "scripts": {
        "prepublishOnly": "cp ../../.npmignore . && cp ../../cessair.js .",
