    "devDependencies": {
        "ava": "^0.22.0",
        "babel-eslint": "^7.2.3",
        "eslint": "^4.5.0",
        "eslint-config-airbnb-base": "^11.3.2",
        "eslint-plugin-import": "^2.7.0",
        "source-map-support": "^0.4.16"
    },
    "engines": {
        "node": ">=6.6.0"
    },
    "ava": {
        "require": "babel-register",
        "babel": "inherit",
        "files": [
            "tests/*.js"
        ]
