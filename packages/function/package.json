{
    "name": "@cessair/function",
    "version": "1.0.6",
    "description": "The interface for extending functions.",
    "homepage": "https://github.com/cessair/cessair#readme",
    "bugs": {
        "url": "https://github.com/cessair/cessair/issues"
    },
    "license": "MIT",
    "author": {
        "name": "Yongbin Min",
        "email": "contact@cichol.com",
