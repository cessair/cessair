        "babel-core": "^7.0.0-bridge.0",
        "babel-eslint": "^8.2.6",
        "babel-jest": "^23.4.2",
        "eslint": "^4.19.1",
        "eslint-config-airbnb-base": "^13.0.0",
        "eslint-plugin-import": "^2.13.0",
        "jest": "^23.4.2"
    },
    "engines": {
        "node": ">=8.11.3",
        "npm": ">=6.3.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "jest": {
        "testEnvironment": "node",
        "testRegex": "/tests/.*\\.js$",
        "verbose": true
    }
