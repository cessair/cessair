{
    "name": "cessair",
    "version": "1.1.4",
    "description": "A catalyst for making robust and stable core of either module or service.",
    "keywords": [
        "cessair",
        "helper",
        "utility",
        "catalyst",
        "extension"
    ],
    "homepage": "https://cessair.github.io/",
    "bugs": {
        "url": "https://github.com/cessair/cessair/issues"
    },
    "license": "MIT",
    "author": {
        "name": "Yongbin Min",
        "email": "contact@cichol.com",
        "url": "https://www.cichol.com/"
    },
    "main": "releases",
    "directories": {
        "lib": "releases",
        "test": "tests"
    },
    "repository": "https://github.com/cessair/cessair/tree/master/packages/cessair",
    "scripts": {
        "prepublishOnly": "cessair generate-npmignore",
        "build": "npm run build:babelrc && npm run build:transpile",
        "build:babelrc": "cessair generate-babelrc",
        "build:transpile": "babel sources --out-dir releases --source-maps inline --quiet",
        "test": "npm run test:eslint",
        "test:eslint": "eslint sources"
    },
    "dependencies": {
        "@cessair/common": "^1.1.6",
        "@cessair/core": "^1.1.6",
        "@cessair/function": "^1.0.0",
        "@cessair/generic": "^1.1.6"
    },
    "devDependencies": {
        "@cessair/building": "^1.2.3",
        "@cessair/cli": "^1.0.4"
    },
    "engines": {
        "node": ">=8.9.4",
        "npm": ">=5.6.0"
    }
}
