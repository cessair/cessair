{
    "private": true,
    "name": "cessair-common",
    "version": "0.0.0",
    "description": "The common features using to implement Cessair.",
    "keywords": [
        "cessair",
        "helper",
        "utility",
        "catalyst",
        "extension"
    ],
    "homepage": "https://github.com/cessair/cessair#readme",
    "bugs": {
