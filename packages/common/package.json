        "name": "Yongbin Min",
        "email": "contact@cichol.com",
        "url": "https://www.cichol.com/"
    },
    "main": "libraries",
    "directories": {
        "lib": "libraries",
        "test": "tests"
    },
    "repository": "https://github.com/cessair/cessair/tree/master/packages/common",
    "scripts": {
        "prepublishOnly": "yarn run bootstrap:copy",
        "postpublish": "yarn run bootstrap:remove",
        "postinstall": "yarn build",
        "prebuild": "yarn run bootstrap:copy",
        "build": "gulp build --silent",
        "postbuild": "yarn run bootstrap:remove",
        "bootstrap:copy": "cp ../../.babelrc . && cp ../../.npmignore . && cp ../../gulpfile.babel.js .",
        "bootstrap:remove": "rm ./.babelrc ./.npmignore ./gulpfile.babel.js"
    },
    "dependencies": {
        "babel-runtime": "^6.25.0",
        "cessair-core": "^0.0.0"
    },
    "devDependencies": {
        "babel-core": "^6.25.0",
        "babel-plugin-syntax-object-rest-spread": "^6.13.0",
        "babel-plugin-transform-async-generator-functions": "^6.24.1",
        "babel-plugin-transform-async-to-generator": "^6.24.1",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-plugin-transform-do-expressions": "^6.22.0",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
        "babel-plugin-transform-exponentiation-operator": "^6.24.1",
        "babel-plugin-transform-export-extensions": "^6.22.0",
        "babel-plugin-transform-function-bind": "^6.22.0",
        "babel-plugin-transform-object-rest-spread": "^6.23.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "gulp": "^3.9.1",
        "gulp-babel": "^7.0.0",
        "gulp-cached": "^1.1.1",
        "gulp-remember": "^0.3.1",
        "gulp-sourcemaps": "^2.6.0"
    },
    "engines": {
        "node": ">=6.6.0",
        "npm": ">=5.3.0",
        "yarn": ">=0.27.5"
    }
}
